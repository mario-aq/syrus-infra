{
  "name": "syrus-infra",
  "version": "0.1.0",
  "bin": {
    "syrus-infra": "bin/syrus-infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy:dev": "npm run build && AWS_PROFILE=arborquote cdk deploy Syrus-dev",
    "deploy:prod": "npm run build && AWS_PROFILE=arborquote cdk deploy Syrus-prod",
    "bootstrap": "AWS_PROFILE=arborquote cdk bootstrap",
    "synth": "npm run build && cdk synth"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.0",
    "aws-cdk": "2.156.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.156.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
